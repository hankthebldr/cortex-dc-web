{
  "displayName": "Cortex DC Web - App Hosting Overview",
  "labels": {
    "team": "cortex-dc-web"
  },
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "service_name",
      "templateVariable": "app_hosting_service",
      "stringValue": "cortex-dc-web"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "region",
      "stringValue": "us-central1"
    }
  ],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "xPos": 0,
        "yPos": 0,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "App Hosting - Request latency (p95)",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "Latency",
                "timeSeriesQuery": {
                  "unitOverride": "ms",
                  "timeSeriesFilter": {
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_PERCENTILE_95",
                      "alignmentPeriod": "60s"
                    },
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" resource.type=\"cloud_run_revision\" resource.label.\"service_name\"=\"cortex-dc-web\""
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Latency (ms)",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 0,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "App Hosting - Request volume",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "STACKED_AREA",
                "legendTemplate": "Requests",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_RATE",
                      "alignmentPeriod": "60s"
                    },
                    "filter": "metric.type=\"run.googleapis.com/request_count\" resource.type=\"cloud_run_revision\" resource.label.\"service_name\"=\"cortex-dc-web\""
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Requests / sec",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 0,
        "yPos": 4,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "App Hosting - Container CPU utilization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "CPU",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN",
                      "alignmentPeriod": "60s"
                    },
                    "filter": "metric.type=\"run.googleapis.com/container/cpu/utilizations\" resource.type=\"cloud_run_revision\" resource.label.\"service_name\"=\"cortex-dc-web\""
                  }
                }
              }
            ],
            "yAxis": {
              "label": "CPU Utilization",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 4,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "App Hosting - Error rate",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "5xx",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_RATE",
                      "alignmentPeriod": "60s"
                    },
                    "filter": "metric.type=\"run.googleapis.com/request_count\" metric.label.\"response_code_class\"=\"5xx\" resource.type=\"cloud_run_revision\" resource.label.\"service_name\"=\"cortex-dc-web\""
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Errors / sec",
              "scale": "LINEAR"
            }
          }
        }
      }
    ]
  }
}
